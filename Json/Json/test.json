{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-0": {
            "": "src/"
        },
        "files": [
            "vendor/_composer_incompatible_packages/illuminate/collections/helpers.php",
            "vendor/_composer_incompatible_packages/illuminate/events/functions.php",
            "vendor/_composer_incompatible_packages/illuminate/support/helpers.php"
        ],
        "psr-4": {
            "Brick\\Math\\": "vendor/_composer_incompatible_packages/brick/math/src",
            "Cron\\": "vendor/_composer_incompatible_packages/dragonmantank/cron-expression/src/Cron",
            "Illuminate\\Bus\\": "vendor/_composer_incompatible_packages/illuminate/bus",
            "Illuminate\\Cache\\": "vendor/_composer_incompatible_packages/illuminate/cache",
            "Illuminate\\Container\\": "vendor/_composer_incompatible_packages/illuminate/container",
            "Illuminate\\Contracts\\": "vendor/_composer_incompatible_packages/illuminate/contracts",
            "Illuminate\\Console\\": [
                "src/Illuminate/BridgeBundle/Backports/Console",
                "vendor/_composer_incompatible_packages/illuminate/console"
            ],
            "Illuminate\\Console\\Scheduling\\": [
                "src/Illuminate/BridgeBundle/Backports/Console/Scheduling",
                "vendor/_composer_incompatible_packages/illuminate/console/Scheduling"
            ],
            "Illuminate\\Events\\": "vendor/_composer_incompatible_packages/illuminate/events",
            "Illuminate\\Database\\": "vendor/_composer_incompatible_packages/illuminate/database",
            "Illuminate\\Foundation\\": "vendor/_composer_incompatible_packages/laravel-zero/foundation/src/Illuminate/Foundation",
            "Illuminate\\Pipeline\\": "vendor/_composer_incompatible_packages/illuminate/pipeline",
            "Illuminate\\Queue\\": "vendor/_composer_incompatible_packages/illuminate/queue",
            "Illuminate\\Redis\\":  "vendor/_composer_incompatible_packages/illuminate/redis",
            "Illuminate\\Support\\": [
                "vendor/_composer_incompatible_packages/illuminate/collections",
                "vendor/_composer_incompatible_packages/illuminate/support"
            ],
            "Illuminate\\Support\\Traits\\": [
                "src/Illuminate/BridgeBundle/Collections/traits",
                "vendor/_composer_incompatible_packages/illuminate/macroable/Traits"
            ],
            "Psr\\Container\\": "vendor/_composer_incompatible_packages/psr/container/src",
            "Ramsey\\Collection\\": "vendor/_composer_incompatible_packages/ramsey/collection/src",
            "Ramsey\\Uuid\\": "vendor/_composer_incompatible_packages/ramsey/uuid/src"
        }
    },
